{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "globals": {
    "React": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "experimentalDecorators" : true
    }
  },

  "plugins": [
    "react",
    "import",
    "babel"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-console": 0,
    "babel/no-invalid-this": 0,
    "babel/object-curly-spacing": 0,
    "babel/quotes": 0,
    "babel/semi": 0,
    "react/no-multi-comp": 0,
    "react/display-name": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "comma-dangle": 0,

  }
}
